<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bg_setting"
    android:layout_height="match_parent"
    tools:context=".livemonitor.setting.YSLiveActivity">

    <include layout="@layout/comment_title"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="427px"
        android:gravity="center_horizontal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="280px"
            android:background="@mipmap/youslogo_blank_bg"
            android:layout_height="238px"/>

        <TextView
            android:layout_width="wrap_content"
            android:textSize="36px"
            android:textColor="@color/white"
            android:layout_marginTop="53px"
            android:text="内容更新中，敬请期待"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>